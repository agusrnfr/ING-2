<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<% if(typeof alert != "undefined"){ %>
    <script>
        Swal.fire({
        title: '<%= alertTitle %>',
        text: '<%= alertMessage %>',
        icon: '<%= alertIcon %>',
        timer: <%= timer %>,
        showConfirmButton: <%= showConfirmButton %>,
        }).then(()=>{
            window.location='/ <%= ruta %>'
        })
    </script>
<% } %>